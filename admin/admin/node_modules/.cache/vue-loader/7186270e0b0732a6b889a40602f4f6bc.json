  </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-else-if=\"ruleForm.yishengzhanghao\"\r\n          label=\"医生账号\"\r\n          prop=\"yishengzhanghao\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.yishengzhanghao\"\r\n            placeholder=\"医生账号\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"upload\"\r\n          v-if=\"type != 'info' && !ro.touxiang\"\r\n          label=\"头像\"\r\n          prop=\"touxiang\"\r\n        >\r\n          <file-upload\r\n            tip=\"点击上传头像\"\r\n            action=\"file/upload\"\r\n            :limit=\"3\"\r\n            :multiple=\"true\"\r\n            :fileUrls=\"ruleForm.touxiang ? ruleForm.touxiang : ''\"\r\n            @change=\"touxiangUploadChange\"\r\n          ></file-upload>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"upload\"\r\n          v-else-if=\"ruleForm.touxiang\"\r\n          label=\"头像\"\r\n          prop=\"touxiang\"\r\n        >\r\n          <img\r\n            v-if=\"ruleForm.touxiang.substring(0, 4) == 'http'\"\r\n            class=\"upload-img\"\r\n            style=\"margin-right:20px;\"\r\n            v-bind:key=\"index\"\r\n            :src=\"ruleForm.touxiang.split(',')[0]\"\r\n            width=\"100\"\r\n            height=\"100\"\r\n          />\r\n          <img\r\n            v-else\r\n            class=\"upload-img\"\r\n            style=\"margin-right:20px;\"\r\n            v-bind:key=\"index\"\r\n            v-for=\"(item, index) in ruleForm.touxiang.split(',')\"\r\n            :src=\"$base.url + item\"\r\n            width=\"100\"\r\n            height=\"100\"\r\n          />\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"性别\"\r\n          prop=\"xingbie\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.xingbie\"\r\n            placeholder=\"性别\"\r\n            clearable\r\n            :readonly=\"ro.xingbie\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          v-else\r\n          class=\"input\"\r\n          label=\"性别\"\r\n          prop=\"xingbie\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.xingbie\"\r\n            placeholder=\"性别\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"医生姓名\"\r\n          prop=\"yishengxingming\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.yishengxingming\"\r\n            placeholder=\"医生姓名\"\r\n            clearable\r\n            :readonly=\"ro.yishengxingming\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          v-else\r\n          class=\"input\"\r\n          label=\"医生姓名\"\r\n          prop=\"yishengxingming\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.yishengxingming\"\r\n            placeholder=\"医生姓名\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"职称\"\r\n          prop=\"zhicheng\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.zhicheng\"\r\n            placeholder=\"职称\"\r\n            clearable\r\n            :readonly=\"ro.zhicheng\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          v-else\r\n          class=\"input\"\r\n          label=\"职称\"\r\n          prop=\"zhicheng\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.zhicheng\"\r\n            placeholder=\"职称\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"挂号费\"\r\n          prop=\"guahaofei\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.guahaofei\"\r\n            placeholder=\"挂号费\"\r\n            clearable\r\n            :readonly=\"ro.guahaofei\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          v-else\r\n          class=\"input\"\r\n          label=\"挂号费\"\r\n          prop=\"guahaofei\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.guahaofei\"\r\n            placeholder=\"挂号费\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"select\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"科室\"\r\n          prop=\"keshi\"\r\n        >\r\n          <el-select\r\n            :disabled=\"ro.keshi\"\r\n            v-model=\"ruleForm.keshi\"\r\n            placeholder=\"请选择科室\"\r\n          >\r\n            <el-option\r\n              v-for=\"(item, index) in keshiOptions\"\r\n              v-bind:key=\"index\"\r\n              :label=\"item\"\r\n              :value=\"item\"\r\n            >\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          v-else\r\n          class=\"input\"\r\n          label=\"科室\"\r\n          prop=\"keshi\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.keshi\"\r\n            placeholder=\"科室\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"上班时间\"\r\n          prop=\"shangbanshijian\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.shangbanshijian\"\r\n            placeholder=\"上班时间\"\r\n            clearable\r\n            :readonly=\"ro.shangbanshijian\"\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          v-else\r\n          class=\"input\"\r\n          label=\"上班时间\"\r\n          prop=\"shangbanshijian\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.shangbanshijian\"\r\n            placeholder=\"上班时间\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"date\"\r\n          v-if=\"type != 'info'\"\r\n          label=\"出诊日期\"\r\n          prop=\"chuzhenriqi\"\r\n        >\r\n          <el-date-picker\r\n            format=\"yyyy 年 MM 月 dd 日\"\r\n            value-format=\"yyyy-MM-dd\"\r\n            v-model=\"ruleForm.chuzhenriqi\"\r\n            type=\"date\"\r\n            :readonly=\"ro.chuzhenriqi\"\r\n            placeholder=\"出诊日期\"\r\n          ></el-date-picker>\r\n        </el-form-item>\r\n        <el-form-item\r\n          :style=\"{ width: '50%', margin: '0 0 20px 0' }\"\r\n          class=\"input\"\r\n          v-else-if=\"ruleForm.chuzhenriqi\"\r\n          label=\"出诊日期\"\r\n          prop=\"chuzhenriqi\"\r\n        >\r\n          <el-input\r\n            v-model=\"ruleForm.chuzhenriqi\"\r\n            placeholder=\"出诊日期\"\r\n            readonly\r\n          ></el-input>\r\n        </el-form-item>\r\n      </template>\r\n      <el-form-item\r\n        :style=\"{ width: '100%', padding: '0', margin: '0' }\"\r\n        class=\"btn\"\r\n      >\r\n        <el-button\r\n          :style=\"{\r\n            border: '0',\r\n            cursor: 'pointer',\r\n            padding: '0',\r\n            margin: '0 20px 0 0',\r\n            outline: 'none',\r\n            color: 'rgba(255, 255, 255, 1)',\r\n            borderRadius: '30px',\r\n            background: '#4662A1',\r\n            width: '128px',\r\n            lineHeight: '40px',\r\n            fontSize: '14px',\r\n            height: '40px',\r\n          }\"\r\n          v-if=\"type != 'info'\"\r\n          type=\"primary\"\r\n          class=\"btn-success\"\r\n          @click=\"onSubmit\"\r\n          >提交</el-button\r\n        >\r\n        <el-button\r\n          :style=\"{\r\n            border: '0px solid rgba(64, 158, 255, 1)',\r\n            cursor: 'pointer',\r\n            padding: '0',\r\n            margin: '0',\r\n            outline: 'none',\r\n            color: '#fff',\r\n            borderRadius: '30px',\r\n            background: '#4662A1',\r\n            width: '128px',\r\n            lineHeight: '40px',\r\n            fontSize: '14px',\r\n            height: '40px',\r\n          }\"\r\n          v-if=\"type != 'info'\"\r\n          class=\"btn-close\"\r\n          @click=\"back()\"\r\n          >取消</el-button\r\n        >\r\n        <el-button\r\n          :style=\"{\r\n            border: '0px solid rgba(64, 158, 255, 1)',\r\n            cursor: 'pointer',\r\n            padding: '0',\r\n            margin: '0',\r\n            outline: 'none',\r\n            color: '#fff',\r\n            borderRadius: '30px',\r\n            background: '#4662A1',\r\n            width: '128px',\r\n            lineHeight: '40px',\r\n            fontSize: '14px',\r\n            height: '40px',\r\n          }\"\r\n          v-if=\"type == 'info'\"\r\n          class=\"btn-close\"\r\n          @click=\"back()\"\r\n          >返回</el-button\r\n        >\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n<script>\r\n// 数字，邮件，手机，url，身份证校验\r\nimport {\r\n  isNumber,\r\n  isIntNumer,\r\n  isEmail,\r\n  isPhone,\r\n  isMobile,\r\n  isURL,\r\n  checkIdCard,\r\n} from \"@/utils/validate\";\r\nexport default {\r\n  data() {\r\n    let self = this;\r\n    var validateIdCard = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!checkIdCard(value)) {\r\n        callback(new Error(\"请输入正确的身份证号码\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    var validateUrl = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!isURL(value)) {\r\n        callback(new Error(\"请输入正确的URL地址\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    var validateMobile = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!isMobile(value)) {\r\n        callback(new Error(\"请输入正确的手机号码\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    var validatePhone = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!isPhone(value)) {\r\n        callback(new Error(\"请输入正确的电话号码\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    var validateEmail = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!isEmail(value)) {\r\n        callback(new Error(\"请输入正确的邮箱地址\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    var validateNumber = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!isNumber(value)) {\r\n        callback(new Error(\"请输入数字\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    var validateIntNumber = (rule, value, callback) => {\r\n      if (!value) {\r\n        callback();\r\n      } else if (!isIntNumer(value)) {\r\n        callback(new Error(\"请输入整数\"));\r\n      } else {\r\n        callback();\r\n      }\r\n    };\r\n    return {\r\n      id: \"\",\r\n      type: \"\",\r\n\r\n      ro: {\r\n        yishengzhanghao: false,\r\n        touxiang: false,\r\n        xingbie: false,\r\n        yishengxingming: false,\r\n        zhicheng: false,\r\n        guahaofei: false,\r\n        keshi: false,\r\n        shangbanshijian: false,\r\n        chuzhenriqi: false,\r\n      },\r\n\r\n      ruleForm: {\r\n        yishengzhanghao: \"\",\r\n        touxiang: \"\",\r\n        xingbie: \"\",\r\n        yishengxingming: \"\",\r\n        zhicheng: \"\",\r\n        guahaofei: \"\",\r\n        keshi: \"\",\r\n        shangbanshijian: \"\",\r\n        chuzhenriqi: \"\",\r\n      },\r\n\r\n      yishengzhanghaoOptions: [],\r\n      keshiOptions: [],\r\n\r\n      rules: {\r\n        yishengzhanghao: [\r\n          { required: true, message: \"医生账号不能为空\", trigger: \"blur\" },\r\n        ],\r\n        touxiang: [],\r\n        xingbie: [],\r\n        yishengxingming: [\r\n          { required: true, message: \"医生姓名不能为空\", trigger: \"blur\" },\r\n        ],\r\n        zhicheng: [],\r\n        guahaofei: [],\r\n        keshi: [],\r\n        shangbanshijian: [],\r\n        chuzhenriqi: [],\r\n      },\r\n    };\r\n  },\r\n  props: [\"parent\"],\r\n  computed: {},\r\n  created() {},\r\n  methods: {\r\n    // 下载\r\n    download(file) {\r\n      window.open(`${file}`);\r\n    },\r\n    // 初始化\r\n    init(id, type) {\r\n      if (id) {\r\n        this.id = id;\r\n        this.type = type;\r\n      }\r\n      if (this.type == \"info\" || this.type == \"else\") {\r\n        this.info(id);\r\n      } else if (this.type == \"logistics\") {\r\n        this.logistics = false;\r\n        this.info(id);\r\n      } else if (this.type == \"cross\") {\r\n        var obj = this.$storage.getObj(\"crossObj\");\r\n        for (var o in obj) {\r\n          if (o == \"yishengzhanghao\") {\r\n            this.ruleForm.yishengzhanghao = obj[o];\r\n            this.ro.yishengzhanghao = true;\r\n            continue;\r\n          }\r\n          if (o == \"touxiang\") {\r\n            this.ruleForm.touxiang = obj[o];\r\n            this.ro.touxiang = true;\r\n            continue;\r\n          }\r\n          if (o == \"xingbie\") {\r\n            this.ruleForm.xingbie = obj[o];\r\n            this.ro.xingbie = true;\r\n            continue;\r\n          }\r\n          if (o == \"yishengxingming\") {\r\n            this.ruleForm.yishengxingming = obj[o];\r\n            this.ro.yishengxingming = true;\r\n            continue;\r\n          }\r\n          if (o == \"zhicheng\") {\r\n            this.ruleForm.zhicheng = obj[o];\r\n            this.ro.zhicheng = true;\r\n            continue;\r\n          }\r\n          if (o == \"guahaofei\") {\r\n            this.ruleForm.guahaofei = obj[o];\r\n            this.ro.guahaofei = true;\r\n            continue;\r\n          }\r\n          if (o == \"keshi\") {\r\n            this.ruleForm.keshi = obj[o];\r\n            this.ro.keshi = true;\r\n            continue;\r\n          }\r\n          if (o == \"shangbanshijian\") {\r\n            this.ruleForm.shangbanshijian = obj[o];\r\n            this.ro.shangbanshijian = true;\r\n            continue;\r\n          }\r\n          if (o == \"chuzhenriqi\") {\r\n            this.ruleForm.chuzhenriqi = obj[o];\r\n            this.ro.chuzhenriqi = true;\r\n            continue;\r\n          }\r\n        }\r\n      }\r\n\r\n      // 获取用户信息\r\n      this.$http({\r\n        url: `${this.$storage.get(\"sessionTable\")}/session`,\r\n        method: \"get\",\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          var json = data.data;\r\